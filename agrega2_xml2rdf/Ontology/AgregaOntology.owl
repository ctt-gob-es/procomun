    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.agrega.es/ont/lom2owl#annotation -->

    <owl:ObjectProperty rdf:about="#annotation">
        <rdfs:comment xml:lang="en"
            >Comments on the educational use of a learning object, containing information on when and by whom the comments were created. This category enables educators to share their assessments of learning objects, suggestions for use, etc. IEEE LOM element 8.</rdfs:comment>
        <rdfs:range rdf:resource="#Annotation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.agrega.es/ont/lom2owl#annotationDate -->

    <owl:ObjectProperty rdf:about="#annotationDate">
        <rdfs:comment xml:lang="en"
            >Date that this annotation was created. IEEE LOM element 8.2.</rdfs:comment>
        <rdfs:domain rdf:resource="#Annotation"/>
        <rdfs:range rdf:resource="#DateTime"/>
    </owl:ObjectProperty>
    


    <!-- http://www.agrega.es/ont/lom2owl#annotationEntity -->

    <owl:ObjectProperty rdf:about="#annotationEntity"/>
    


    <!-- http://www.agrega.es/ont/lom2owl#classification -->

    <owl:ObjectProperty rdf:about="#classification">
        <rdfs:comment xml:lang="en"
            >Description of where a learning object falls within a particular classification system. IEEE LOM element 9.</rdfs:comment>
        <rdfs:range rdf:resource="#Classification"/>
    </owl:ObjectProperty>
    


    <!-- http://www.agrega.es/ont/lom2owl#classificationKeyword -->

    <owl:ObjectProperty rdf:about="#classificationKeyword">
        <rdfs:comment rdf:datatype="&xsd;string"
            >Keywords and phrases descriptive of the learning object relative to the stated 9.1:Classification.Purpose of this specific classification, such as accessibility, security level, etc., most relevant first. IEEE LOM element 9.4</rdfs:comment>
        <rdfs:domain rdf:resource="#Classification"/>
    </owl:ObjectProperty>
    


    <!-- http://www.agrega.es/ont/lom2owl#classificationTaxon -->

    <owl:ObjectProperty rdf:about="#classificationTaxon">
        <rdfs:comment xml:lang="en"
            >A particular term within a taxonomy which serves to classify a learning object. IEEE LOM element 9.2.2.</rdfs:comment>
        <rdfs:range rdf:resource="#Taxon"/>
        <rdfs:domain rdf:resource="#TaxonPath"/>
    </owl:ObjectProperty>
    


    <!-- http://www.agrega.es/ont/lom2owl#contributionDateTime -->

    <owl:ObjectProperty rdf:about="#contributionDateTime">
        <rdfs:comment xml:lang="en"
            >The date of the contribution. IEEE LOM element 3.2.3.</rdfs:comment>
        <rdfs:domain rdf:resource="#Contribution"/>
        <rdfs:range rdf:resource="#DateTime"/>
    </owl:ObjectProperty>
    


    <!-- http://www.agrega.es/ont/lom2owl#contributionEntity -->

    <owl:ObjectProperty rdf:about="#contributionEntity">
        <rdfs:comment rdf:datatype="&xsd;string"
            >The identification of and information about entities (i.e., people, organizations) contributing to this learning object. The entities shall be ordered as most relevant first. IEEE LOM element 3.2.2.</rdfs:comment>
        <rdfs:domain rdf:resource="#Contribution"/>
        <rdfs:range rdf:resource="#VCard"/>
    </owl:ObjectProperty>
    


    <!-- http://www.agrega.es/ont/lom2owl#coverage -->

    <owl:ObjectProperty rdf:about="#coverage">
        <terms:coverage rdf:datatype="&rdfs;Literal"></terms:coverage>
        <rdfs:comment xml:lang="en"
            >The time, culture, geography or region to which this learning object applies.
The extent or scope of the content of the learning object. Coverage will typically include spatial location (a place name or geographic coordinates), temporal period (a period label, date, or date range) or jurisdiction (such as a named administrative entity). Recommended best practice is to select a value from a controlled vocabulary (for example, the Thesaurus of Geographic Names [TGN]) and that, where appropriate, named places or time periods be used in preference to numeric identifiers such as sets of coordinates or date ranges. IEEE LOM element 1.6.</rdfs:comment>
        <rdfs:domain rdf:resource="#LearningObject"/>
    </owl:ObjectProperty>
    


    <!-- http://www.agrega.es/ont/lom2owl#description -->

    <owl:ObjectProperty rdf:about="#description">
        <terms:description rdf:datatype="&rdfs;Literal"></terms:description>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Comments on how this learning object is to be used. IEEE LOM element 5.10</rdfs:comment>
        <rdfs:comment xml:lang="en"
            >A textual description of the content of this learning object. IEEE LOM element 1.4.</rdfs:comment>
        <rdfs:domain rdf:resource="#LearningObject"/>
    </owl:ObjectProperty>
    


    <!-- http://www.agrega.es/ont/lom2owl#duration -->

    <owl:ObjectProperty rdf:about="#duration">
        <rdfs:domain rdf:resource="#Duration"/>
    </owl:ObjectProperty>
    


    <!-- http://www.agrega.es/ont/lom2owl#educational -->

    <owl:ObjectProperty rdf:about="#educational">
        <rdfs:comment rdf:datatype="&xsd;string"
            >This category describes the key educational or pedagogic characteristics of this learning object. IEEE LOM element 5.</rdfs:comment>
        <rdfs:range rdf:resource="#Educational"/>
        <rdfs:domain rdf:resource="#LearningObject"/>
    </owl:ObjectProperty>
    


    <!-- http://www.agrega.es/ont/lom2owl#educationalTypicalAgeRange -->

    <owl:ObjectProperty rdf:about="#educationalTypicalAgeRange">
        <rdf:value rdf:datatype="&rdfs;Literal"></rdf:value>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Age of the typical intended user. IEEE LOM element 5.7.</rdfs:comment>
        <rdfs:domain rdf:resource="#Educational"/>
    </owl:ObjectProperty>
    


    <!-- http://www.agrega.es/ont/lom2owl#educationalTypicalLearningTime -->

    <owl:ObjectProperty rdf:about="#educationalTypicalLearningTime">
        <rdfs:range rdf:resource="#Duration"/>
        <rdfs:domain rdf:resource="#Educational"/>
    </owl:ObjectProperty>
    


    <!-- http://www.agrega.es/ont/lom2owl#hasRelatedResource -->

    <owl:ObjectProperty rdf:about="#hasRelatedResource">
        <rdfs:comment xml:lang="en"
            >The target learning object that a relationship references. IEEE LOM element 7.2.</rdfs:comment>
        <rdfs:range rdf:resource="#Identifier"/>
        <rdfs:domain rdf:resource="#Relation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.agrega.es/ont/lom2owl#hasResourceIdentifier -->

    <owl:ObjectProperty rdf:about="#hasResourceIdentifier">
        <rdfs:comment xml:lang="en"
            >A globally unique label that identifies the target  learning object. IEEE LOM element 7.2.1.</rdfs:comment>
        <rdfs:range rdf:resource="#Identifier"/>
    </owl:ObjectProperty>
    


    <!-- http://www.agrega.es/ont/lom2owl#identifier -->

    <owl:ObjectProperty rdf:about="#identifier">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:comment xml:lang="en"
            >This predicate represents the fact that a learning object has a label that identifies it. IEEE LOM element 1.1.</rdfs:comment>
        <rdfs:range rdf:resource="#Identifier"/>
        <rdfs:domain rdf:resource="#LearningObject"/>
    </owl:ObjectProperty>
    


    <!-- http://www.agrega.es/ont/lom2owl#includesSingleLangString -->

    <owl:ObjectProperty rdf:about="#includesSingleLangString">
        <rdfs:comment xml:lang="en"
            >LangString values may include multiple semantically equivalent character strings, such as translations or alternative descriptions. Each of these is represented by a singleLangString.</rdfs:comment>
        <rdfs:range rdf:resource="#LangString"/>
    </owl:ObjectProperty>
    


    <!-- http://www.agrega.es/ont/lom2owl#isCompositeOf -->

    <owl:ObjectProperty rdf:about="#isCompositeOf">
        <rdfs:comment xml:lang="en"
            >When grouping multiple requirements, the composite requirement is satisfied when one of the component requirements is satisfied, i.e., the logical connector is OR. IEEE LOM element 4.4.1.</rdfs:comment>
        <rdfs:range rdf:resource="#SingleTechnicalRequirement"/>
        <rdfs:domain rdf:resource="#TechnicalRequirement"/>
    </owl:ObjectProperty>
    


    <!-- http://www.agrega.es/ont/lom2owl#keyword -->

    <owl:ObjectProperty rdf:about="#keyword">
        <rdf:value rdf:datatype="&rdfs;Literal"></rdf:value>
        <rdfs:comment xml:lang="en"
            >A keyword or phrase describing the topic of this learning object. This data element should not be used for characteristics that can be described by other data elements. IEEE LOM element 1.5.</rdfs:comment>
        <rdfs:domain rdf:resource="#LearningObject"/>
    </owl:ObjectProperty>
    


    <!-- http://www.agrega.es/ont/lom2owl#lifeCycleContribution -->

    <owl:ObjectProperty rdf:about="#lifeCycleContribution"/>
    


    <!-- http://www.agrega.es/ont/lom2owl#metaMetadataContribution -->

    <owl:ObjectProperty rdf:about="#metaMetadataContribution">
        <rdfs:comment xml:lang="en"
            >Those entities (i.e., people, organizations) that have contributed to the state of this learning object during its life cycle (e.g.,creation, edits, publication).</rdfs:comment>
        <rdfs:domain rdf:resource="#LearningObject"/>
        <rdfs:range rdf:resource="#LifeCycleContribution"/>
    </owl:ObjectProperty>
    


    <!-- http://www.agrega.es/ont/lom2owl#metaMetadataIdentifier -->

    <owl:ObjectProperty rdf:about="#metaMetadataIdentifier">
        <rdfs:comment rdf:datatype="&xsd;string"
            >A globally unique label that identifies this metadata record. IEEE LOM element 3.1.</rdfs:comment>
        <rdfs:range rdf:resource="#Identifier"/>
        <rdfs:domain rdf:resource="#LearningObject"/>
    </owl:ObjectProperty>
    


    <!-- http://www.agrega.es/ont/lom2owl#relatedResourceDescription -->

    <owl:ObjectProperty rdf:about="#relatedResourceDescription">
        <terms:description rdf:datatype="&rdfs;Literal"></terms:description>
        <rdfs:range rdf:resource="#LangString"/>
        <rdfs:domain rdf:resource="#Relation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.agrega.es/ont/lom2owl#relation -->

    <owl:ObjectProperty rdf:about="#relation">
        <rdfs:comment xml:lang="en"
            >Relationship between a learning object and other learning objects, if any.</rdfs:comment>
        <rdfs:range rdf:resource="#Relation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.agrega.es/ont/lom2owl#taxonPath -->

    <owl:ObjectProperty rdf:about="#taxonPath">
        <rdfs:comment xml:lang="en"
            >A taxonomic path in a specific classification system according to which a learning object is classified. IEEE LOM element 9.2.</rdfs:comment>
        <rdfs:domain rdf:resource="#Classification"/>
        <rdfs:range rdf:resource="#TaxonPath"/>
    </owl:ObjectProperty>
    


    <!-- http://www.agrega.es/ont/lom2owl#technicalDuration -->

    <owl:ObjectProperty rdf:about="#technicalDuration">
        <rdfs:domain rdf:resource="#Duration"/>
        <rdfs:range rdf:resource="#LearningObject"/>
    </owl:ObjectProperty>
    


    <!-- http://www.agrega.es/ont/lom2owl#technicalRequirements -->

    <owl:ObjectProperty rdf:about="#technicalRequirements">
        <rdfs:comment xml:lang="en"
            >A technical capability necessary for using a learning object. IEEE LOM Element &quot;4.4. Requirement&quot;.</rdfs:comment>
        <rdfs:range rdf:resource="#TechnicalRequirement"/>
    </owl:ObjectProperty>
    



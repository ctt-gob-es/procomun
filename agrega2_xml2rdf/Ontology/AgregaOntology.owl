    </owl:DatatypeProperty>
    


    <!-- http://www.agrega.es/ont/lom2owl#educationalCognitiveProcess -->

    <owl:DatatypeProperty rdf:about="#educationalCognitiveProcess">
        <rdfs:comment
            >Cognitive Process</rdfs:comment>
        <rdfs:domain rdf:resource="#Educational"/>
        <rdfs:range>
            <rdf:Description>
                <rdf:type rdf:resource="&owl;DataRange"/>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="&rdf;List"/>
                        <rdf:first rdf:datatype="&xsd;string">auniversal</rdf:first>
                        <rdf:rest rdf:resource="&rdf;nil"/>
                    </rdf:Description>
                </owl:oneOf>
            </rdf:Description>
        </rdfs:range>
        <rdfs:range>
            <rdf:Description>
                <rdf:type rdf:resource="&owl;DataRange"/>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="&rdf;List"/>
                        <rdf:first rdf:datatype="&xsd;string">universal</rdf:first>
                        <rdf:rest rdf:resource="&rdf;nil"/>
                    </rdf:Description>
                </owl:oneOf>
            </rdf:Description>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://www.agrega.es/ont/lom2owl#educationalContext -->

    <owl:DatatypeProperty rdf:about="#educationalContext">
        <rdfs:comment xml:lang="en"
            >The principal environment within which the learning and use of this learning object is intended to take place. IEEE LOM element 5.6.</rdfs:comment>
        <rdfs:domain rdf:resource="#Educational"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
        <rdfs:range rdf:resource="&xsd;short"/>
        <rdfs:range>
            <rdf:Description>
                <rdf:type rdf:resource="&owl;DataRange"/>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="&rdf;List"/>
                        <rdf:first rdf:datatype="&xsd;string"
                            >higher education</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="&rdf;List"/>
                                <rdf:first rdf:datatype="&xsd;string">other</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="&rdf;List"/>
                                        <rdf:first rdf:datatype="&xsd;string">school</rdf:first>
